CREATE TABLE `your_project.your_dataset.customer_data`
(
  customer_id STRING,
  first_name STRING,
  last_name STRING,
  full_name STRING,
  email STRING,
  phone1 STRING,
  phone2 STRING,
  gender STRING,
  date_of_birth DATE,
  subscription_date DATE,
  country STRING,
  city STRING,
  address STRING,
  company STRING,
  job_title STRING,
  department STRING,
  industry STRING,
  website STRING,
  acct_number STRING,
  eid_number STRING,
  password STRING,
  last_login TIMESTAMP,
  account_created DATE,
  year_joined INT64,
  loyalty_points INT64,
  referral_code STRING,
  preferred_language STRING,
  alternate_email STRING,
  membership_tier INT64,
  is_premium BOOL,
  support_tickets INT64,
  last_ticket_date DATE,
  marketing_opt_in BOOL,
  monthly_spend NUMERIC,
  lifetime_value NUMERIC,
  churn_risk NUMERIC,
  net_promoter_score INT64,
  region STRING,
  segment STRING,
  timezone STRING,
  linkedin_profile STRING,
  twitter_handle STRING,
  facebook_profile STRING,
  ip_address STRING,
  device_type STRING,
  last_purchase_date DATE,
  total_orders INT64,
  avg_order_value NUMERIC,
  recent_feedback STRING,
  preferred_contact_method STRING
)
PARTITION BY _PARTITIONTIME
OPTIONS(
  description="Customer dataset partitioned by ingestion time"
);

